<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - backup_website</title>
  <link rel="shortcut icon" href="~/img/logo/sansiri-logo-sm-white.png" />
  @await Html.PartialAsync("_Styles")
  <base href="/" />
</head>



<body data-sidebar="dark">

  <div id="layout-wrapper">

    @await Html.PartialAsync("partials/_Navbar")
    @await Html.PartialAsync("partials/_Sidebar")

    <!-- Start right Content here -->
    <div class="main-content">
      <div class="page-content">
        <div class="container-fluid">

          <!-- start page title -->
          <div class="page-title-box">
            <div class="row align-items-center">
              <div class="col-md-8">
                <h6 class="page-title">@ViewData["Title"]</h6>
                <ol class="breadcrumb m-0">
                  <li class="breadcrumb-item"><a href="#">Veltrix</a></li>
                  <li class="breadcrumb-item"><a href="#">Tables</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Data tables</li>
                </ol>
              </div>
            </div>
          </div>
          <!-- end page title -->


          <div class="row">
            <div class="col-12">
              <div class="card">
                <div class="card-body p-5">
                  @RenderBody()
                </div>
              </div>
            </div>
          </div>


        </div>
      </div>
      <!-- End Page-content -->

      @await Html.PartialAsync("partials/_Footer")
    </div>
    <!-- end main content-->

  </div>

  <!-- Right Sidebar -->
  @await Html.PartialAsync("partials/_SidebarSettings")
  <!-- /Right-bar -->

  <!-- Right bar overlay-->
  <div class="rightbar-overlay"></div>

  @await Html.PartialAsync("_Scripts")
  @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
